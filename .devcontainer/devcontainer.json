{
    "name": "homebrew-radex",
    "image": "homebrew/brew:latest",
    "runArgs": [
        "--name=homebrew-radex"
    ],
    "customizations": {
        "vscode": {
            "extensions": [
                "github.vscode-pull-request-github"
            ],
            "settings": {
                "files.insertFinalNewline": true,
                "files.trimTrailingWhitespace": true
            }
        }
    }
}
